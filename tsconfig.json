{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
      "outDir": "./js/",
      "rootDir": "./src",
      "sourceMap": true,
      "strict": false,
      "noImplicitReturns": true,
      "noImplicitAny": true,
      "module": "es6",
      "moduleResolution": "node",
      "target": "es5",
      "allowJs": true,
  },
  "include": [
      "./src/**/*"
  ]
}